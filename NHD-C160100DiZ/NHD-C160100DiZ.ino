void I2C_Stop() //I2C Stop Condition 
{
  digitalWrite(SDA, LOW);
  digitalWrite(SCL, LOW);
  digitalWrite(SCL, HIGH);
  digitalWrite(SDA, HIGH);
}

void I2C_out(unsigned char i) //I2C Output 
{
  int n;
  unsigned char j;
  j=i;
  for (n=0;n<8;n++){            // Send 8 bits
    if((j&0x80)==0x80)          // Only get MSB
    digitalWrite(SDA, HIGH);    // SDA = 1
    else
    digitalWrite(SDA, LOW);     // SDA = 0
    j=(j<<1);                   // Shift Data Byte Left
    digitalWrite(SCL, LOW);     
    digitalWrite(SCL, HIGH);    // Clock In Data
    digitalWrite(SCL, LOW);
  }
  digitalWrite(SCL, HIGH);
  while(SDA==1){                // Wait until ACK
    digitalWrite(SCL, LOW);
    digitalWrite(SCL, HIGH);
  }
  digitalWrite(SCL, LOW);
}

void disp(unsigned char *pic) //Function to show image
{
  int n,i;
  unsigned char page = 0xB0; 
  I2C_Start();              // Start Condition
  I2C_out(slave2w);         // Slave Address
  I2C_out(comsend);         // Byte = Command
  I2C_out(0xAF);            // Display ON
  for (i=0;i<13;i++){       // 100 pixel display / 8 pixels = 12.5 pages => Round up to 13 pages. 
    I2C_Start();
    I2C_out(slave2w);
    I2C_out(comsend);
    I2C_out(page);
    I2C_out(0x10);
    I2C_out(0x01);
    I2C_Stop();
    I2C_Start();
    I2C_out(slave2w);
    I2C_out(datasend);      // Byte = Data
    for (n=0;n<160;n++){    // 160 Columns wide
      I2C_out(*pic);        // Write Picture Data
      I2C_out(*pic);        // Write Picture Data
      I2C_out(*pic);        // Write Picture Data
      I2C_out(*pic);        // Write Picture Data
      pic++;
    }
    I2C_Stop();             // Stop Condition
    page++;                 // After 160 columns, go to next page 
  }
}

